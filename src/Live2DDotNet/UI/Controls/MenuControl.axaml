<UserControl
    x:Class="Live2DDotNet.UI.Controls.BaseMenuControl"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:model="clr-namespace:Live2DDotNet.UI.Model"
    xmlns:model1="clr-namespace:Live2DDotNet.UI.Model.Items"
    xmlns:setting="clr-namespace:Live2DDotNet.Utils"
    xmlns:svg="clr-namespace:Live2DDotNet.UI.Views.Svg"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="model:MenuModel"
    mc:Ignorable="d">
    <Panel>
        <DockPanel Name="TopPanel">
            <Panel Name="TabPanel" ClipToBounds="True">
                <Decorator Name="Content1" />
                <Decorator Name="Content2" />
            </Panel>
        </DockPanel>
        <Border
            Name="SidePanel3"
            Background="#00FFFFFF"
            IsVisible="False">
            <Border
                HorizontalAlignment="Left"
                VerticalAlignment="Stretch"
                Background="{setting:Theme TopBGColor}">
                <Decorator Name="SidePanel2" />
            </Border>
        </Border>
    </Panel>
</UserControl>
